    <SolidColorBrush x:Key="Gray" Color="#FF898989"/>
    <SolidColorBrush x:Key="Red" Color="#FFE40404"/>
    <SolidColorBrush x:Key="Orange" Color="#FFF75832"/>
    <SolidColorBrush x:Key="ThemeGray" Color="#FF767676"/>
    <SolidColorBrush x:Key="ThemeBlueAlt" Color="#FFE5EAEB"/>
    <SolidColorBrush x:Key="ThemeSeparator" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="ThemeButtonAlt.Background" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="ThemeScrollThumb" Color="#FF8C8C8C"/>
    <SolidColorBrush x:Key="ThemeScrollThumbOver" Color="#FF000000"/>
