<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2">


    <SolidColorBrush x:Key="ThemeBlue" Color="#FF153B50"/>
    <SolidColorBrush x:Key="DisabledThemeBlue" Color="#FF51788E"/>
    <SolidColorBrush x:Key="ThemeHighlightBlue" Color="#FF0E4F73"/>
    <SolidColorBrush x:Key="ThemeBorder" Color="#FFC4C4C4"/>
    <SolidColorBrush x:Key="ThemeBorderAlt" Color="#FF241D1D"/>
    <SolidColorBrush x:Key="White" Color="#FFFFFFFF"/>
    <!--<SolidColorBrush x:Key="Blue" Color="#FF0676B3"/>-->
    <SolidColorBrush x:Key="Blue" Color="#FF0366D6"/>
    <SolidColorBrush x:Key="DisabledBlue" Color="#FF50A1D6"/>
